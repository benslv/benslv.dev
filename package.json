  "dependencies": {
    "@fontsource/inter": "^4.2.2",
    "gatsby": "^3.2.1",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-styled-components": "^4.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "eslint-plugin-react": "^7.23.1",
